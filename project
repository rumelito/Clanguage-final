                                            int aa, bb, cc;
                                            p("\n   * P O S T * I N C R E M E N T *\n");
                                            p("=====================================\n\n");
                                            printf("Input the value of X: ");
                                            scanf(" %d", &xx);
                                            printf("Input the value of Y: ");
                                            scanf(" %d", &yy);
                                            printf("Input the value of Z: ");
                                            scanf(" %d", &zz);

                                            // use post increment operator to update the value by 1

                                            aa = xx++;
                                            bb = yy++;
                                            cc = zz++;

                                            printf(" \nThe original value of the A: %d ", aa);
                                            printf(" \nThe original value of the B: %d ", bb);
                                            printf(" \nThe original value of the C: %d \n", cc);
                                            printf(" \nThe upadated value of the X: %d ", xx);
                                            printf(" \nThe upadated value of the Y: %d ", yy);
                                            printf(" \nThe upadated value of the Z: %d \n", zz);
                                            break;
                                            case 3:
                                            system("cls");
                                            p("\n   * P R E - D E C R E M E N T *\n");
                                            p("===================================\n\n");
                                            printf("Input the value of X: ");
                                            scanf(" %d", &xx);
                                            printf("Input the value of Y: ");
                                            scanf(" %d", &yy);
                                            printf("Input the value of Z: ");
                                            scanf(" %d", &zz);

                                            // use pre increment operator to update the value by 1

                                            --xx;
                                            --yy;
                                            --zz;

                                            printf(" \nThe upadated value of the X: %d ", xx);
                                            printf(" \nThe upadated value of the Y: %d ", yy);
                                            printf(" \nThe upadated value of the Z: %d \n", zz);

                                            break;
                                            case 4:
                                            system("cls");
                                            p("\n   * P O S T * D E C R E M E N T *\n");
                                            p("=====================================\n\n");
                                            printf("Input the value of X: ");
                                            scanf(" %d", &xx);
                                            printf("Input the value of Y: ");
                                            scanf(" %d", &yy);
                                            printf("Input the value of Z: ");
                                            scanf(" %d", &zz);

                                            // use post increment operator to update the value by 1

                                            aa = xx--;
                                            bb = yy--;
                                            cc = zz--;

                                            printf(" \nThe original value of the A: %d ", aa);
                                            printf(" \nThe original value of the B: %d ", bb);
                                            printf(" \nThe original value of the C: %d \n ", cc);
                                            printf(" \nThe upadated value of the X: %d ", xx);
                                            printf(" \nThe upadated value of the Y: %d ", yy);
                                            printf(" \nThe upadated value of the Z: %d \n", zz);
                                            break;

                                    }
                                   break;
                                    }
                                break;
                        case 6:
                            system("cls");
                            float quiz, assign, proj, cs, me, grades;
                            char section[50], year[50];

                                p("|------------- GRADE COMPUTATION -------------|\n\n");

                                p("Name: ");
                                s(" %100[^\n]", name);

                                p("Section: ");
                                s("%s", section);

                                p("Year: ");
                                s("%s", year);

                                p("\nEnter your Quiz Grade: ");
                                s("%f", &quiz);

                                p("Enter your Assignment Grade: ");
                                s("%f", &assign);

                                p("Enter your Project Grade: ");
                                s("%f", &proj);

                                p("Enter your Class Standing Grade: ");
                                s("%f", &cs);

                                p("Enter your Major Exam Grade: ");
                                s("%f", &me);

                                grades = (cs*.10)+(quiz*.15)+(assign*.15)+(proj*.20)+(me*.40);

                                system("cls");

                                p("\nYour grade is %.2f ", grades);

                                if(grades>=75)
                                {
                                    p("\n\nCongratulations, %s from %s %s, You passed this semester\n\n", name, section, year);
                                }

                                if(grades<75)
                                {
                                    p("\n\nI'm sorry, %s from %s %s, You failed this semester\n\n", name, section, year);
                                }

                            break;

