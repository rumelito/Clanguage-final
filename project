                            printf("\n\n\t\t\t\t\tLoading\n\t\t\t\t\t");
                            for (ss = 0; ss<42; ss++)
                            {
                                Sleep(1);
                                printf("%c", 219);
                            }
                            system("cls");
                            p("\n\t\t  * C O N D I T I O N A L * S T A T E M E N T S *\n");
