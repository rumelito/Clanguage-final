                                    scanf(" %d", &a);
                                    printf("Input the value of B: ");
                                    scanf(" %d", &b);

                                    if (z==w && a!=b)
                                    {
                                    printf("\nX is less than to Y AND A is equal to B\n");
                                    }

                                    if(z==w || a==b)
