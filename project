                                    p("\t\t%d pc Beef & Sausage Overload    -   Php %.2f\n", qtys, totalamount);
                                    p("\t\t\t\t\t\t\t\t \n");
                                    p("\t\tTotal amount:                       Php %.2f \n", overall_amount);
                                    p("\t\tChange amount:                      Php %.2f \n", change);
                                    p("\t\t===================================================\n");
                                    p("\t\t %s ", name);p("\t%s", time);p("\t\t %s \n", date);
                                    p("\t\t===================================================\n");
                                    p("\n\t\tYour order will be ready in 10 minutes...\n");
                                    p("\n\t\tTHANK YOU <3 \n");
                                }
                        }
                    if(yesno=='N'||yesno=='n')
                        {
                            p("\n\t\tMoney: ");
                            s("%f", &mny);
                            change = (mny - amount);

                            system("cls");
                            p("\n\t\t             P I Z Z A L I C I O U S            \n");
                            p("\n\t\t          * R * E * C * E * I * P * T *         \n");
                            p("\n\t\t                 T A K E - O U T                \n");
                            p("\t\t===================================================\n");
                            p("\t\tYour Order:                           Amount   \n");
                            p("\t\t%d pc Beef & Sausage Overload    -   Php %.2f\n", qty, amount);
                            p("\t\t\t\t\t\t\t\t \n");
                            p("\t\tTotal amount:                       Php %.2f \n", amount);
                            p("\t\tChange amount:                      Php %.2f \n", change);
                            p("\t\t===================================================\n");
                            p("\t\t %s ", name);p("\t%s", time);p("\t\t %s \n", date);
                            p("\t\t===================================================\n");
                            p("\n\t\tYour order will be ready in 10 minutes...\n");
                            p("\n\t\tTHANK YOU <3 \n");

                        }
        }
    else{
            p("\n\t\tNot Available!\n");
        }
        }
    if(num==3){
            p("\n\t\tYou have been Exit!\n");
        }
break;


                        case 9:
                            printf("\n\n\t\t\t\t\tLoading\n\t\t\t\t\t");
                            for (ss = 0; ss<42; ss++)
                            {
                                Sleep(1);
                                printf("%c", 219);
                            }

                            system("cls");
                            p("\n\t\t          * L * O * O * P * I * N * G *          \n");
                            p("\t\t===================================================\n");
                            p("\t\t||[1] FOR LOOP                                   ||\n");
                            p("\t\t||                                               ||\n");
                            p("\t\t||[2] WHILE LOOP                                 ||\n");
                            p("\t\t||                                               ||\n");
                            p("\t\t||[3] DO-WHILE LOOP                              ||\n");
                            p("\t\t||                                               ||\n");
                            p("\t\t===================================================\n\n");

                            p("\t\tSELECT A NUMBER: ");
                            s("%d", &num);

                            switch(num){
                            case 1:
                                printf("\n\t\tLoading\n\t\t");

                            for (ss = 0; ss<21; ss++)
                            {
                                Sleep(1);
                                printf("%c", 219);
                            }
                                system("cls");
                                p("\n\t\t           * F * O * R * L * O * O * P *         \n");
                                p("\t\t===================================================\n");
                                p("\t\t||[1] FOR LOOP INCREMENT                         ||\n");
                                p("\t\t||                                               ||\n");
                                p("\t\t||[2] FOR LOOP DECREMENT                         ||\n");
                                p("\t\t||                                               ||\n");
                                p("\t\t||[3] FOR LOOP TRIANGLE                          ||\n");
                                p("\t\t||                                               ||\n");
                                p("\t\t||[4] FOR LOOP INVERTED TRIANGLE                 ||\n");
                                p("\t\t||                                               ||\n");
                                p("\t\t===================================================\n\n");

                                p("\t\tSELECT A NUMBER: ");
                                s("%d", &num);

                                switch(num){
                                case 1:
                                    system("cls");
                                    int x, y;
                                    p("\n   * F O R * L O O P * I N C R E M E N T *\n");
                                    p("=============================================\n\n");
                                    printf("Enter the Starting value: ");
                                    scanf("%d", &x);
                                    printf("Enter the Ending value: ");
                                    scanf("%d", &y);

                                for (x; x<=y; x++){

                                    printf("%d \n", x);
                                }
                                break;
                                case 2:
                                    system("cls");
                                    p("\n   * F O R * L O O P * D E C R E M E N T *\n");
                                    p("=============================================\n\n");
                                    printf("Enter the Starting value: ");
                                    scanf("%d", &x);
                                    printf("Enter the Ending value: ");
                                    scanf("%d", &y);

                                for (x; x>=y; x--){

                                    printf("%d \n", x);
                                }
                                break;

                                case 3:
                                     system("cls");
                                     int rows, i, j;
                                     p("\n   * F O R * L O O P * T R I A N G L E *\n");
                                     p("===========================================\n\n");
                                     printf("Enter the number of rows: ");
                                     scanf("%d", &rows);

                                     for(i=1; i<=rows; i++)
                                     {
                                         for (j = 1; j<=rows - i; j++)
                                         {
                                             printf(" ");
                                         }

                                         for(j=1; j<=i; j++)
                                         {
                                             printf("* ");
                                         }
                                         printf("\n");
                                     }
                                    break;

                                case 4:
                                     system("cls");
                                        //read the number from the user
                                        p("\n   * F O R * L O O P * I N V E R T E D * T R I A N G L E *\n");
                                        p("=============================================================\n\n");
                                        printf("Enter the number of rows: ");
                                        scanf("%d", &rows);

                                        for(i=rows; i>=1; i--)
                                        {
                                            //Print spaces
                                            for (j = 1; j<=rows - i; j++)
                                            {
                                                printf(" ");
                                            }
                                            //Print asterisks
                                            for(j = 1; j<=i; j++)
                                            {
                                                printf("* ");
                                            }
                                            //Move to the next line
                                            printf("\n");
                                     }
                                    break;

                                }
                                break;

                            case 2:
                                system("cls");
                                int i, e;
                                p("\n   * W H I L E * L O O P *\n");
                                p("=============================\n\n");
                                printf("Enter the Starting value: ");
                                s("%i", &i);
                                printf("Enter the Ending value: ");
                                s("%i", &e);

                                while(i <= e){
                                    printf("%i\n", i);
                                    i++;
                                }
                                break;

                            case 3:
                                system("cls");
                                p("\n   * D O - W H I L E * L O O P *\n");
                                p("===================================\n\n");
                                printf("Enter the Starting value: ");
                                s("%i", &i);
                                printf("Enter the Ending value: ");
                                s("%i", &e);

                                do{
                                    printf("%i\n", i);
                                    i++;

                                }while (i<=e);

                                break;

                            }
                            break;

                        case 10:
                            system("cls");
                            p("\nYou have been Exit!\n");
                            return 0;
                            break;

                        default:
                                system("cls");
                                printf("\nENTER A CORRECT NUMBER!\n");
                                break;
                    }

                           p("\ntype B to back to menu: ");
                            s("%s", &back);
                            }while (back=='B' || back=='b');
    }

                    if(yesno=='N'||yesno=='n'){
                        system("cls");
                        p("\nTHANK YOU!\n");
                    }

    return 0;
}
