                            p("\t\t||                                               ||\n");
                            p("\t\t||[4] COLOR SELECTION                            ||\n");
                            p("\t\t||                                               ||\n");
                            p("\t\t===================================================\n\n");

                            p("\t\tSELECT A NUMBER: ");
                            s("%d", &num);

                            switch(num){
                                int number;
                                char color;
                                case 1:
                                    system("cls");
                                    p("\n  * Q U A L I F I E D * T O * V O T E *\n");
                                    p("=========================================\n\n");
                                    printf("Enter your Age: ");
                                    scanf("%i", &age);

                                    if(age>=18){
                                        printf("\nQualified to Vote\n");
                                    }
                                    else {
                                        printf("\nToo Young!\n");
                                    }
                                break;
                                case 2:
                                    system("cls");
                                    p("\n   * P O S I T I V E * O R * N E G A T I V E *\n");
                                    p("=================================================\n\n");
                                    printf("Enter the Number: ");
                                    scanf("%i", &number);
                                    if(number>=0){
                                        printf("\nPOSITIVE NUMBER\n");
                                    }
                                    else {
                                        printf("\nNEGATIVE NUMBER\n");
                                    }
                                break;
                                case 3:
                                    system("cls");
                                    float grade;
                                    p("\n   * G R A D E * R A N G E *\n");
                                    p("===============================\n\n");
                                    printf("Enter your Grade: ");
                                    scanf("%f", &grade);

                                    if(grade>=90 && grade<=100){
                                        printf("\nGRADE: A\n");
                                    }
                                    else if(grade>=80 && grade<90){
                                        printf("\nGRADE: B\n");
                                    }
                                    else if(grade>=70 && grade<80){
                                        printf("\nGRADE: C\n");
                                    }
                                    else if(grade>=60 && grade<70){
                                        printf("\nGRADE: D\n");
                                    }
                                    else if(grade>=0 && grade<60){
                                        printf("\nGRADE: E\n");
                                    }
                                    else {
                                        printf("\nOUT OF RANGE!\n");
                                    }
                                break;
                                case 4:
                                    system("cls");
                                    p("\n    * C O L O R * S E L E C T I O N *\n");
                                    p("=========================================\n\n");
                                    printf("Enter your Color: ");
                                    scanf(" %c", &color);

                                    if(color=="B" || color=='b'){
                                        printf("\nBlue\n");
                                    }
                                    else if(color=="R" || color=='r'){
                                        printf("\nRed\n");
                                    }
                                    else if(color=="G" || color=='g'){
                                        printf("\nGreen\n");
                                    }
                                    else if(color=="Y" || color=='y'){
                                        printf("\nYellow\n");
                                    }
                                    else {
                                        printf("\nUnknown Color\n");
                                    }
                                break;

                            }
                            break;



